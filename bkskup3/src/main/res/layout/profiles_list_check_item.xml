<?xml version="1.0" encoding="utf-8"?>


<com.bk.layout.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/profilesListItem"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@drawable/checkable_background_red"
    android:gravity="center_vertical"
    android:minHeight="@dimen/list_row_height"
    >

    <TextView
        android:id="@+id/profileName"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        style="@style/list_cell"
        />


</com.bk.layout.CheckableLinearLayout>


